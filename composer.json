{
  "name": "root/www",
  "description": "URL helper lib",
  "type": "library",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Denis Belov"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^5.23"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm",
    "check": [
      "@test",
      "@psalm"
    ]
  }
}
